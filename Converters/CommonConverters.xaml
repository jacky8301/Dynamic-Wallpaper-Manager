<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:WallpaperEngine.Converters">
    <local:BooleanToColorConverter x:Key="BooleanToColorConverter" />
    <local:FavoriteIconConverter x:Key="FavoriteIconConverter" />
    <local:ImagePathToSourceConverter x:Key="ImageToSourceConverter" />
    <local:InverseBooleanConverter x:Key="InverseBooleanConverter" />
    <local:IntEqualConverter x:Key="EqualToZeroConverter" Target="0" />
    <local:IntEqualConverter x:Key="EqualToOneConverter" Target="1" />
    <local:IntEqualConverter x:Key="EqualToTwoConverter" Target="2" />
    <local:FavoriteTextConverter x:Key="FavoriteTextConverter" />
</ResourceDictionary>